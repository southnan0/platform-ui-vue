{
  "name": "oa",
  "version": "0.1.4",
  "description": "oa",
  "author": "southnan0",
  "license": "MIT",
  "main": "lib/oa-ui.common.js",
  "unpkg": "lib/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue src examples packages --fix --quiet",
    "bootstrap": "yarn || npm i",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "clean": "rimraf lib && rimraf packages/*/lib && rimraf test/**/coverage",
    "build:file": "node build/bin/build-entry.js & node build/bin/version.js",
    "dist": "npm run clean && npm run build:file && npm run lint && webpack --config build/webpack.conf.js && webpack --config build/webpack.common.js && webpack --config build/webpack.component.js && npm run build:utils",
    "dev": "npm run bootstrap && npm run build:file && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.demo.js",
    "test": "npm run lint &&  cross-env CI_ENV=/dev/ BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test:watch": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js"
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-stage-2": "^7.8.3",
    "@babel/register": "^7.12.1",
    "@form-create/element-ui": "^1.0.20",
    "axios": "^0.20.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.3",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "element-ui": "2.13.0",
    "eslint": "^7.8.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.1.0",
    "highlight.js": "^10.1.2",
