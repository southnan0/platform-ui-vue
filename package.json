    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "throttle-debounce": "^2.3.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "1.1.2",
    "vue": "2.6.10",
